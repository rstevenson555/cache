package com.bos.cache;

import com.bos.cache.impl.MRUData;

public class NonExpiringFactory extends CacheDataFactory {
    private static NonExpiringFactory instance = new NonExpiringFactory();

    public MRUData create(Object key,Object value)
    {
        return new NonExpiringData(key,value);
    }

    public static CacheDataFactory getInstance()
    {
        return instance;
    }
}

